{#
/**
 * @file qiz_quiz.html.twig
 * Default theme implementation to present Qiz quiz data.
 *
 * This template is used when viewing Qiz quiz pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_qiz_quiz()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('qiz_quiz') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
